<div class="labelJarPortlet"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="ftw.labels">

    <ul class="labelJarPortletListing">
        <li class="labelJarPortletListingItem" tal:repeat="label view/labels"
            tal:attributes="style string:color=${label/color}">
            <spantal:content="label/title">
        </li>
    </ul>
</div>
